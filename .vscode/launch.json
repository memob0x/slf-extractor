{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Go",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "--experimental-specifier-resolution=node"
            ],
            "program": "${workspaceRoot}/index.js",
            "args": [
                "data/Ambient.slf",
            ],
            "console": "integratedTerminal"
        }
    ]
}